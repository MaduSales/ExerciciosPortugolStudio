programa {
  funcao inicio() {
    //Descubra se o número é ímpar ou par
    inteiro numero

    escreva("Digite um número para saber se ele é par ou ímpar: ")
    leia(numero)

    se (numero % 2 == 0){
      escreva("O número é par")
    }
    senao{
      escreva("O número é ímpar")
    }

  }
}
